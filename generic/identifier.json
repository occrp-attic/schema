{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.occrp.org/generic/identifier.json#",
  "title": "Identifier",
  "description": "An issued identifier",
  "type": "object",
  "inline": true,
  "properties": {
    "identifier": {
      "title": "Identifier",
      "description": "An issued identifier, e.g. a DUNS number",
