{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://schema.occrp.org/generic/land.json#",
  "title": "Land",
  "description": "A parcel of land",
  "type": "object",
  "oneOf": [
      {"$ref": "http://schema.occrp.org/generic/asset.json#"}
  ],
  "properties": {
    "parcel_number": {
        "type": ["string", "null"]
    },
    "parcel_name": {
        "type": ["string", "null"]
    },
    "parcel_area": {
        "type": ["integer", "null"]
    },
    "parcel_area_units": {
        "type": ["string", "null"]
    },
    "usage_code": {
        "type": ["string", "null"]
    },
    "usage_name": {
        "type": ["string", "null"]
    }
  }
}
